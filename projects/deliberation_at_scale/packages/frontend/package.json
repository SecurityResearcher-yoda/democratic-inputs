    "@supabase/auth-helpers-nextjs": "0.7.4",
    "@supabase/supabase-js": "2.32.0",
    "autoprefixer": "10.4.14",
    "dayjs": "1.11.9",
    "next": "13.4.13",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "dotenv-cli": "7.2.1",
    "encoding": "0.1.13",
